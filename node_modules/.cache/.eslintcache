rvaciones\"\r\n                    type=\"text\"\r\n                    name=\"Obser\"\r\n                    onChange={formik.handleChange}\r\n                  />\r\n                </label>\r\n                <h6 style={{ color: \"red\" }}>{formik.errors.Obser}</h6>\r\n              </Col>\r\n            </div>\r\n          </div>\r\n\r\n          <div className=\"col-md-3\" id=\"Container-1\">\r\n            <label>\r\n              <Row>\r\n                <input\r\n                  className=\"Row\"\r\n                  type=\"text\"\r\n                  name=\"Firma_vend\"\r\n                  onChange={formik.handleChange}\r\n                ></input>\r\n                <p>Firma</p>\r\n              </Row>\r\n              <h6 style={{ color: \"red\" }}>{formik.errors.Firma_vend}</h6>\r\n            </label>\r\n          </div>\r\n          <div className=\"col-md-12\">\r\n            <Button type=\"submit\" className=\"BOTON\" variant=\"primary\">\r\n              Guardar\r\n            </Button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default PersonaJuridica;\r\n","C:\\Users\\crisu\\Desktop\\Presentacion_prueba\\src\\pages\\Private\\Director\\Formulario_Director.js",[],"C:\\Users\\crisu\\Desktop\\Presentacion_prueba\\src\\pages\\Private\\Asesor\\Persona_Natural\\GeneralPN.js",[],"C:\\Users\\crisu\\Desktop\\Presentacion_prueba\\src\\pages\\Public\\Inicio_sesion\\InicioS.js",["397","398","399","400","401","402","403","404","405","406","407"],"import React, { useState, useRef, useContext } from \"react\";\r\nimport {\r\n  Formulario,\r\n  ContenedorBotonCentrado,\r\n  Boton,\r\n  MensajeError,\r\n} from \"./Elementos/Formularios\";\r\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\r\nimport { faExclamationTriangle } from \"@fortawesome/free-solid-svg-icons\";\r\nimport Input from \"./ComponentesInicio/Input\";\r\nimport \"./InicioS.css\";\r\nimport logo from \"../../../assets/Logo/